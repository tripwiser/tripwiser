{
  "expo": {
    "name": "vibecode",
    "slug": "vibecode",
    "scheme": "vibecode",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vibecode.tripkit"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.vibecode.tripkit"
    },
    "plugins": [
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsDownloadToken": "YOUR_MAPBOX_ACCESS_TOKEN"
        }
      ],
      "expo-font",
      "expo-splash-screen",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location to show nearby places on the map."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "5baed529-01a3-44fd-98ad-f9552550392a"
      }
    }
  }
}
